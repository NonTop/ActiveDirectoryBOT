TOKEN=token						                                 # Изменить на свой ТОКЕН
LDAP_SERVER=ldaps://ip                           			     # Изменить на свой айпи сервера
LDAP_BIND_USER=CN=name_account,OU=OU,OU=OU,DC=dc,DC=dc,DC=dc     # Изменить на свой логин
LDAP_BIND_PASSWORD=password                                	     # Изменить на свой пароль от логина
LDAP_BASE_DN=DC=dc,DC=dc,DC=dc                             	     # Изменить на свою раскладку


